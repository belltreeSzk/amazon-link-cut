{
  "manifest_version": 2,
  "name": "Amazon Link Shortener",
  "version": "1.0.0",
  "description": "Amazonのリンクを短くして、好きなアフィリエイトタグを追加します",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}